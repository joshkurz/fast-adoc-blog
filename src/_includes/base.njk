<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1"/>
  <title>{{ title or "fast-adoc-blog" }}</title>
  <link rel="stylesheet" href="{{ '/styles.css' | asset }}"/>
</head>
{% set gTheme = siteConfig.theme %}
<body{% if gTheme %} data-theme="{{ gTheme }}"{% endif %}>
  <header class="wrap">
    <h1><a href="/">fast-adoc-blog</a></h1>
    <nav>
      <a href="/">Home</a>
      {% if env.isLocal %} · <a href="/setup">Setup</a>{% endif %}
    </nav>
  </header>

  <main class="wrap">
    {{ content | safe }}
  </main>

  <footer class="wrap">
    <small>Built with 11ty + AsciiDoc · Comments by giscus/Waline</small>
  </footer>
</body>
</html>
